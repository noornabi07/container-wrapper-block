{
  "apiVersion": "1",
  "name": "ctrb/container",
  "title": "Container Block",
  "description": "This is a wrapper block for other blocks within it.",
  "category": "common",
  "keywords": [
    "container",
    "wrapper",
    "gutenberg",
    "container-block",
    "wrapper-block"
  ],
  "textdomain": "container-block",
  "attributes": {
    "align": {
      "type": "string",
      "default": "none"
    },
    "cId": {
      "type": "string",
      "default": ""
    },
    "content": {
      "type": "string",
      "default": "I am bangladeshi horny boy. Please support me"
    },
    "tab": {
      "type": "string",
      "default": {
        "activeTab": "tab1"
      }
    },
    "columns": {
      "type": "object",
      "default": {
        "width": { "desktop": "", "tablet": "", "mobile": "" }
      }
    },
    "heightColumns": {
      "type": "object",
      "default": {
        "height": { "desktop": "", "tablet": "", "mobile": "" }
      }
    },
    "marginColumns": {
      "type": "object",
      "default": {
        "margin": { "desktop": {}, "tablet": {}, "mobile": {} }
      }
    },
    "paddingColumns": {
      "type": "object",
      "default": {
        "padding": {
          "desktop": {
            "top": "10px",
            "right": "10px",
            "bottom": "10px",
            "left": "10px"
          },
          "tablet": {
            "top": "10px",
            "right": "10px",
            "bottom": "10px",
            "left": "10px"
          },
          "mobile": {
            "top": "10px",
            "right": "10px",
            "bottom": "10px",
            "left": "10px"
          }
        }
      }
    },
    "border": {
      "type": "object",
      "default": {
        "radius": "5px"
      }
    },
    "background": {
      "type": "object",
      "default": {
        "normalBg": { "color": "#fff" },
        "hoverBg": { "color": "#fff" },
        "transition": 0.5
      }
    },
    "overlyBackground": {
      "type": "object",
      "default": {
        "oNormalBg": { "color": "#fff" },
        "oHoverBg": { "color": "#fff" },
        "normalOpacity": 0.3,
        "hoverOpacity": 0.3,
        "hoverTransition": 0.4
      }
    },
    "actionStyleButton": {
      "type": "string",
      "default": "normal"
    },
    "overlyStyleButton": {
      "type": "string",
      "default": "normal"
    },
    "borderStyleButton": {
      "type": "string",
      "default": "normal"
    },
    "isWrapper": {
      "type": "boolean",
      "default": false
    },
    "isHeight": {
      "type": "boolean",
      "default": false
    },
    "contentAlign": {
      "type": "string",
      "default": "top"
    },
    "shapedButton": {
      "type": "string",
      "default": "top"
    },
    "shaped": {
      "type": "object",
      "default": {
        "topShaped": "none",
        "bottomShaped": "none",
        "topColors": "#006769",
        "bottomColors": "#006769"
      }
    },
    "shapedColumns": {
      "type": "object",
      "default": {
        "topWidth": {
          "desktop": "100%",
          "tablet": "",
          "mobile": ""
        },
        "bottomWidth": {
          "desktop": "100%",
          "tablet": "",
          "mobile": ""
        },
        "topHeight": {
          "desktop": "75px",
          "tablet": "",
          "mobile": ""
        },
        "bottomHeight": {
          "desktop": "75px",
          "tablet": "",
          "mobile": ""
        }
      }
    },
    "isTopFlip": {
      "type": "boolean",
      "default": false
    },
    "isBottomFlip": {
      "type": "boolean",
      "default": false
    },
    "isTopFront": {
      "type": "boolean",
      "default": false
    },
    "isBottomFront": {
      "type": "boolean",
      "default": false
    },
    "isOverly": {
      "type": "boolean",
      "default": false
    },
    "blendType": {
      "type": "string",
      "default": "normal"
    },
    "isFilterCss": {
      "type": "boolean",
      "default": false
    },
    "wrapAlignment": {
      "type": "string",
      "default": "none"
    }
  },

  "supports": {
    "align": ["wide", "full"],
    "html": false
  },
  "example": {
    "attributes": {
      "preview": true,
      "columns": {
        "desktop": 1,
        "tablet": 1,
        "mobile": 1
      }
    }
  },
  "editorScript": "file:./../dist/editor.js"
}
